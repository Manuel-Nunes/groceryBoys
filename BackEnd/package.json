{
  "name": "grocery-boys",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.17",
    "body-parser": "^1.20.2",
    "express": "^4.18.2"
  },
  "scripts": {
    "start:BuildAndGo": "tsc && cd ./compiled && node server.js",
    "start": "nodemon",
    "build": "tsc"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run start:BuildAndGo",
    "ext": "ts"
  },
  "type": "module",
  "devDependencies": {
    "@types/node": "^20.5.0",
    "eslint": "^8.47.0",
    "eslint-plugin-import": "^2.28.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
